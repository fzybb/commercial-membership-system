andler(e, model) {\n      e.preventDefault();\n      //调用注册接口\n      registerApi(model.phoneValue, model.pwdValue, model.nameValue).then(\n        res => {\n          if (res.data.code === 0) {\n            const toast = this.$createToast({\n              txt: \"注册成功\",\n              type: \"correct\",\n              time: 1500\n            });\n            toast.show();\n          }\n        }\n      );\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n.main {\n  padding: 50px 5% 0;\n  text-align: center;\n}\n//注册\n.cube-btn {\n  margin-top: 20px;\n}\n// 登录\n.reg {\n  display: inline-block;\n  margin-top: 30px;\n  font-size: 18px;\n}\n</style>"],"mappings":"AAyBA;AACA,SAAAA,WAAA;AACA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,KAAA;QACAC,UAAA;QACAC,QAAA;QACAC,SAAA;MACA;MACAC,MAAA;QACAC,IAAA;QACAC,QAAA;QACAC,KAAA;QACAC,KAAA;UACAC,WAAA;QACA;QACAC,KAAA;UACAC,QAAA;QACA;QACAC,QAAA;UACAD,QAAA;QACA;MACA,GACA;QACAN,IAAA;QACAC,QAAA;QACAC,KAAA;QACAC,KAAA;UACAC,WAAA;QACA;QACAC,KAAA;UACAC,QAAA;QACA;QACAC,QAAA;UACAD,QAAA;QACA;MACA,GACA;QACAN,IAAA;QACAC,QAAA;QACAC,KAAA;QACAC,KAAA;UACAC,WAAA;UACAJ,IAAA;UACAQ,GAAA;YACAC,IAAA;UACA;QACA;QACAJ,KAAA;UACAC,QAAA;QACA;QACAC,QAAA;UACAD,QAAA;QACA;MACA;IAGA;EACA;EACAI,OAAA;IACAC,aAAA,WAAAA,cAAAC,CAAA,EAAAjB,KAAA;MAAA,IAAAkB,KAAA;MACAD,CAAA,CAAAE,cAAA;MACA;MACArB,WAAA,CAAAE,KAAA,CAAAC,UAAA,EAAAD,KAAA,CAAAE,QAAA,EAAAF,KAAA,CAAAG,SAAA,EAAAiB,IAAA,CACA,UAAAC,GAAA;QACA,IAAAA,GAAA,CAAAtB,IAAA,CAAAuB,IAAA;UACA,IAAAC,KAAA,GAAAL,KAAA,CAAAM,YAAA;YACAC,GAAA;YACApB,IAAA;YACAqB,IAAA;UACA;UACAH,KAAA,CAAAI,IAAA;QACA;MACA,CACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}